import java.util.*;     
  public class AverageOfList  
  {  
    public static void main(String args[])   
    {  
      int sum = 0, avg;      
      ArrayList<Integer> list = new ArrayList<Integer>();    
      //adding elements to the List  
      list.add(1);  
      list.add(3);  
      list.add(1);  
      list.add(4);  
      list.add(6);  
      //do while loop iterates over the List  

    int i = 0;
      do{
     //getting elements from the list and adding to the variable sum   
      sum = sum + list.get(i); 
      }while(i<list.size());
       
      //finds the average of the list of numbers
      avg = sum / list.size();   
      //prints the result  
      System.out.println("The average of the List of the numbers: " + avg);  
    }  
  }  
